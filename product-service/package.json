{
  "name": "product-service",
  "version": "0.1.0",
  "bin": {
    "product-service": "bin/product-service.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:deploy": "cdk deploy --require-approval never",
    "cdk:destroy": "cdk destroy --force",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "db:write": "aws dynamodb batch-write-item --request-items file://src/mocks/productList.json"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "20.1.7",
    "@types/uuid": "^9.0.2",
    "aws-cdk": "2.81.0",
    "esbuild": "^0.17.19",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.82.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.82.0-alpha.0",
    "@aws-sdk/client-dynamodb": "^3.350.0",
    "aws-cdk-lib": "2.81.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "dotenv": "^16.1.4",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  }
}
